# Oracle Database Configuration (Production)
ORACLE_USER=your-username
ORACLE_PASSWORD=your-password
ORACLE_HOST=your-oracle-host
ORACLE_PORT=1521
ORACLE_SID=your-service-name
ORACLE_SERVICE=your-service-name

# ========== RAG и векторная БД (совместимо с sql4A) ==========

# Модель эмбеддингов (как в sql4A - хорошо работает с русским языком)
# Варианты: intfloat/multilingual-e5-base (768d), intfloat/multilingual-e5-large (1024d), cointegrated/rubert-tiny2 (312d)
EMBEDDING_MODEL=intfloat/multilingual-e5-base

# Qdrant настройки (векторная БД для KB)
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=kb_billing

# Oracle Schema для billing (используется для выполнения SQL запросов)
# Схема billing содержит таблицы STECCOM: SPNET_TRAFFIC, STECCOM_EXPENSES, SERVICES, ANALYTICS и др.
ORACLE_SCHEMA=billing

# Настройки поиска (как в sql4A)
DEFAULT_SEARCH_LIMIT=5
SIMILARITY_THRESHOLD=0.7
EMBEDDING_BATCH_SIZE=32

# Настройки генерации SQL
MAX_CONTEXT_EXAMPLES=5
MAX_CONTEXT_TOKENS=2000

# OpenAI API для генерации SQL через proxyapi.ru (как в sql4A)
# Если не установлено, система будет показывать только похожие примеры
OPENAI_API_KEY=your-proxyapi-api-key
OPENAI_API_BASE=https://api.proxyapi.ru/openai/v1
OPENAI_MODEL=gpt-3.5-turbo

# Логирование
LOG_LEVEL=INFO

